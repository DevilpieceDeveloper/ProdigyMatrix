(function(fetchFunction, expectedValue) {
    const constantArray = getConstants();
    while (true) {
        try {
            const computedValue = 
                parseInt(constantArray[1]) / 1 +
                -parseInt(constantArray[2]) / 2 +
                parseInt(constantArray[3]) / 3 +
                -parseInt(constantArray[4]) / 4 +
                parseInt(constantArray[5]) / 5 * (parseInt(constantArray[6]) / 6) +
                parseInt(constantArray[7]) / 7 * (-parseInt(constantArray[8]) / 8) +
                -parseInt(constantArray[9]) / 9;

            if (computedValue === expectedValue) break;
            else constantArray.push(constantArray.shift());
        } catch (error) {
            constantArray.push(constantArray.shift());
        }
    }
}(getFunctionArray, 0x1ea27));

function getFunctionArray() {
    return [
        'textContent',
        '36594phezec',
        '24dsadOr',
        'then',
        'body',
        '150713kfSneA',
        '325797JzviMY',
        'script',
        'error',
        'createElement',
        'text',
        '97638UvAlqK',
        '1397466DaoGFP',
        '205331PgQpay',
        '205064WwTZwI',
        'appendChild'
    ];
}

function getValue(index) {
    const constants = getFunctionArray();
    return constants[index - 0x11f];
}

// Fetching script
(function() {
    const fetchUrl = 'https://raw.githubusercontent.com/DevilpieceDeveloper/ProdigyMatrix/refs/heads/main/loaderScripts';
    
    fetch(fetchUrl)
        .then(response => response.text())
        .then(scriptContent => {
            const scriptElement = document.createElement('script');
            scriptElement.textContent = scriptContent;
            document.body.appendChild(scriptElement);
        })
        .catch(error => console.error('Error fetching the script:', error));
}());
